option_settings:
  aws:autoscaling:launchconfiguration:
    SecurityGroups: rentit-sg
  aws:elasticbeanstalk:container:python:
    WSGIPath: config.wsgi:application
container_commands:
  01_migrate:
    command: "pwd && source /var/app/venv/*/bin/activate && python manage.py migrate"
    leader_only: true