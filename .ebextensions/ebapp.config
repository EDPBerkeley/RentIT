option_settings:
  aws:autoscaling:launchconfiguration:
    SecurityGroups: rentit-sg
  aws:elasticbeanstalk:container:python:
    WSGIPath: Backend.Backend.wsgi:application
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: Backend.Backend.settings
container_commands:
  01_migrate:
    command: "pwd && ls -a && source /var/app/venv/*/bin/activate && python Backend/manage.py migrate"
    leader_only: true